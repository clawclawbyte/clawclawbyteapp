{
  "extraKnownMarketplaces": {
    "sawyerhood/dev-browser": {
      "source": {
        "source": "github",
        "repo": "sawyerhood/dev-browser"
      }
    }
  },
  "enabledPlugins": {
    "dev-browser@dev-browser-marketplace": true
  },
